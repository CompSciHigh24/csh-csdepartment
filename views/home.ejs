	<div class="thirdbgimg">
			<div class="caption"> <span class="  bg-transparent text-25 text-white">
					<div class="hide  swiper-Imgs">
						<!-- Additional required wrapper -->
						<div class="swiper-wrapper">
							<!-- Slides -->
							<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./Background:Carousel Images/APCSA Class-2024.png" width="50%" height="10%"></div>
							
							<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./images/_team_Halloween.jpg" width="50%" height="50%"></div>

							<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./images/_students_LyricAlex.jpg" width="50%" height="30%"></div>

							<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./Background:Carousel Images/IMG_6210.png" width="25%" height="10%"></div>
							<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./images/_students_HackPrinceton2.jpg" width="25%" height="10%"></div>
							
							<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./Background:Carousel Images/IMG_4822.png" width="25%" height="10%"></div>

							
							<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./Background:Carousel Images/IMG_9064.png" width="50%" height="10%"></div>
							<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./images/_team_Halloween2.jpg" width="50%" height="10%"></div>
<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./images/CS-Fair-1.jpeg" width="50%" height="10%"></div>
<div  class="swiper-slide cursor-pointer justify-center m-auto"><img onclick="openLightbox(this)" class="justify-center m-auto transform transition duration-500 hover:scale-105" src="./images/CS-Fair-3.jpg" width="50%" height="10%"></div>

						</div>
						<!-- If we need pagination -->
						<div class="my-10">
							<div class="swiper-pagination"></div>
						</div>
						

						
						<div class="swiper-button-prev"></div>
						<div class="swiper-button-next"></div>

						
						
					</div>
			</span> </div>
